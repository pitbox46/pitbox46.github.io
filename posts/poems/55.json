{
    "number": "55",
    "title": "For Andrew",
    "author": "Mason Griswold",
    "date": "2023-01-01",
    "poem": [
        "Rockets fire through skies",
        "Emancipating streaks of fire",
        "Crippling sounds of black powder",
        "Eerie cries of a mother's baby",
        "",
        "In the dirt is a locket",
        "An earring",
        "And a token",
        "",
        "A locket of a bride, frozen",
        "An earring of a heart, pulsing",
        "A token of a dream, hidden",
        "",
        "Rockets tumble down skies",
        "Emancipating streaks of fire",
        "Crippling sounds of black powder",
        "Eerie cries of a mother"
    ]
}